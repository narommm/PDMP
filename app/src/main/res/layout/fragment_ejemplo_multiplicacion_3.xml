<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ejemploMultiplicacion.ejemplo_multiplicacion_3">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ejemploMultiplicacion3"
        android:background="@drawable/fondo_principal">

        <ImageView
            android:id="@+id/audio3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@android:drawable/ic_lock_silent_mode_off"
            app:layout_constraintBottom_toTopOf="@+id/imview_signo_mult"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.952"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.831" />

        <ImageView
            android:id="@+id/imageView70"
            android:layout_width="166dp"
            android:layout_height="148dp"
            android:layout_marginTop="60dp"
            android:src="@drawable/verde_pregunta_cuatro"
            app:layout_constraintEnd_toStartOf="@+id/imageView72"
            app:layout_constraintHorizontal_bias="0.213"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imview_signo_mult" />

        <ImageView
            android:id="@+id/imageView77"
            android:layout_width="166dp"
            android:layout_height="148dp"
            android:src="@drawable/anara_pregunta_uno"
            app:layout_constraintEnd_toStartOf="@+id/imageView78"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView76" />

        <ImageView
            android:id="@+id/imageView78"
            android:layout_width="166dp"
            android:layout_height="148dp"
            android:layout_marginEnd="68dp"
            android:src="@drawable/anara_pregunta_dos"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView76" />

        <ImageView
            android:id="@+id/imageView72"
            android:layout_width="166dp"
            android:layout_height="148dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="4dp"
            android:src="@drawable/ama_pregunta_tres"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imview_signo_mult" />

        <ImageView
            android:id="@+id/imview_signo_mult"
            android:layout_width="121dp"
            android:layout_height="96dp"
            android:src="@drawable/signo_multiplicacion"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_contenido_multiplicacion" />


        <TextView
            android:id="@+id/txt_contenido_multiplicacion"
            android:layout_width="311dp"
            android:layout_height="90dp"
            android:fontFamily="@font/pacifico"
            android:text="@string/txt_multi"
            android:textColor="@color/blanco"
            android:textSize="@dimen/medium_1_50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.048" />

        <ImageView
            android:id="@+id/imageView73"
            android:layout_width="123dp"
            android:layout_height="110dp"
            android:layout_marginTop="70dp"
            android:src="@drawable/signo_multiplicacion_ejemplo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imview_signo_mult" />

        <ImageView
            android:id="@+id/imageView76"
            android:layout_width="347dp"
            android:layout_height="43dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/raya_ejemplo_multiplicacion"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.376"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView73"
            app:layout_constraintVertical_bias="0.00999999" />

        <ImageView
            android:id="@+id/imageView79"
            android:layout_width="182dp"
            android:layout_height="120dp"
            android:layout_marginBottom="104dp"
            android:src="@drawable/dialogo_multiplicador"
            app:layout_constraintBottom_toBottomOf="@+id/imageView72"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView72"
            app:layout_constraintTop_toTopOf="@+id/imview_signo_mult"
            app:layout_constraintVertical_bias="0.68" />

        <ImageView
            android:id="@+id/imageView80"
            android:layout_width="182dp"
            android:layout_height="120dp"
            android:layout_marginTop="88dp"
            android:src="@drawable/dialogo_producto"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView76" />

        <ImageView
            android:id="@+id/imageView81"
            android:layout_width="168dp"
            android:layout_height="120dp"
            android:layout_marginBottom="104dp"
            android:src="@drawable/dialogo_multiplicando"
            app:layout_constraintBottom_toBottomOf="@+id/imageView70"
            app:layout_constraintEnd_toEndOf="@+id/imageView70"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_contenido_multiplicacion"
            app:layout_constraintVertical_bias="0.68" />

        <ImageView
            android:id="@+id/btn_siguiente"
            android:layout_width="279dp"
            android:layout_height="94dp"
            android:src="@drawable/btn_siguiente"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView80"
            app:layout_constraintVertical_bias="0.86" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>