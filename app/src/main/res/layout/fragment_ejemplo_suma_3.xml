<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".EjemploSuma3">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ejemploSuma3"
        android:background="@drawable/fondo_principal">


        <ImageView
            android:id="@+id/audio8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:src="@android:drawable/ic_lock_silent_mode_off"
            app:layout_constraintBottom_toTopOf="@+id/imview_signo_mult"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.923"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imview_icon_suma"
            android:layout_width="121dp"
            android:layout_height="96dp"
            android:layout_marginBottom="584dp"
            android:src="@drawable/signo_suma"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/txt_contenido_suma"
            android:layout_width="167dp"
            android:layout_height="73dp"
            android:fontFamily="@font/pacifico"
            android:text="@string/txt_suma"
            android:textColor="@color/blanco"
            android:textSize="@dimen/medium_1_50dp"
            app:layout_constraintBottom_toTopOf="@+id/imview_icon_suma"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.594"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/view_uno"
            android:layout_width="158dp"
            android:layout_height="112dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="56dp"
            android:src="@drawable/naran_num_uno"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imview_icon_suma" />

        <ImageView
            android:id="@+id/view_nueve"
            android:layout_width="158dp"
            android:layout_height="112dp"
            android:layout_marginTop="56dp"
            android:src="@drawable/naran_num_nueve"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/view_uno"
            app:layout_constraintTop_toBottomOf="@+id/imview_icon_suma" />

        <ImageView
            android:id="@+id/view_dos"
            android:layout_width="158dp"
            android:layout_height="112dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="60dp"
            android:src="@drawable/verde_num_dos"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_uno" />

        <ImageView
            android:id="@+id/view_cuatro"
            android:layout_width="158dp"
            android:layout_height="112dp"
            android:src="@drawable/verde_num_cuatro"
            app:layout_constraintBottom_toTopOf="@+id/linea_blanca"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/view_dos"
            app:layout_constraintTop_toBottomOf="@+id/view_nueve"
            app:layout_constraintVertical_bias="0.714" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:src="@drawable/eje_suma_3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view_cuatro_answer"
            app:layout_constraintTop_toBottomOf="@+id/linea_blanca"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/simple_signo_resta"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view_dos"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.594" />

        <ImageView
            android:id="@+id/linea_blanca"
            android:layout_width="283dp"
            android:layout_height="34dp"
            android:layout_marginTop="24dp"
            android:src="@drawable/linea_blanca"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.578"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_dos" />

        <ImageView
            android:id="@+id/view_cuatro_answer"
            android:layout_width="158dp"
            android:layout_height="112dp"
            android:layout_marginStart="32dp"
            android:src="@drawable/b_numero_cuatro"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linea_blanca"
            app:layout_constraintVertical_bias="0.32" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="158dp"
            android:layout_height="112dp"
            android:layout_marginBottom="44dp"
            android:src="@drawable/b_numero_tres"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView13"
            app:layout_constraintTop_toBottomOf="@+id/linea_blanca"
            app:layout_constraintVertical_bias="0.774" />

        <TextView
            android:id="@+id/number_ejm_suma_3"
            style="@style/textGame"
            android:layout_width="49dp"
            android:layout_height="47dp"
            android:fontFamily="@font/autour_one"
            android:text="1"
            android:textColor="@color/blanco"
            android:textSize="@dimen/medium_3_35dp"
            app:layout_constraintBottom_toTopOf="@+id/view_uno"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.295"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imview_icon_suma"
            app:layout_constraintVertical_bias="1.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>