            android:layout_width="match_parent"
            android:layout_height="102dp"
            android:fontFamily="@font/pacifico"
            android:text="@string/txt_division"
            android:textColor="@color/negro"
            android:textSize="60dp"
            android:gravity="center"
            app:layout_constraintBottom_toTopOf="@+id/imview_icon_suma"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.608" />

        <ImageView
            android:id="@+id/imageView55"
            android:layout_width="162dp"
            android:layout_height="132dp"
            android:layout_marginTop="64dp"
            android:layout_marginEnd="160dp"
            android:src="@drawable/verde_pregunta_cinco"
            app:layout_constraintBottom_toTopOf="@+id/respuesta_2"
            app:layout_constraintEnd_toEndOf="@+id/imageView56"
            app:layout_constraintTop_toBottomOf="@+id/txt_contenido_suma2"
            app:layout_constraintVertical_bias="0.181" />

        <ImageView
            android:id="@+id/imageView62"
            android:layout_width="162dp"
            android:layout_height="132dp"
            android:layout_marginTop="72dp"
