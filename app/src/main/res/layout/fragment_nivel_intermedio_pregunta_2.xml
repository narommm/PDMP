            app:layout_constraintTop_toBottomOf="@+id/txt_contenido_suma2" />

        <ImageView
            android:id="@+id/imageView56"
            android:layout_width="162dp"
            android:layout_height="132dp"
            android:layout_marginTop="72dp"
            android:src="@drawable/ama_pregunta_cinco"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_contenido_suma2" />

        <ImageView
            android:id="@+id/respuesta_2"
            android:layout_width="162dp"
            android:layout_height="132dp"
            android:layout_marginTop="44dp"
            android:src="@drawable/anara_signo_interrogacion"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.654"
            app:layout_constraintStart_toEndOf="@+id/respuesta_1"
            app:layout_constraintTop_toBottomOf="@+id/imageView56" />

        <ImageView
            android:id="@+id/respuesta_1"
            android:layout_width="162dp"
            android:layout_height="132dp"
            android:layout_marginStart="168dp"
            android:layout_marginTop="44dp"
            android:src="@drawable/anara_signo_interrogacion"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView56" />

        <ImageView
            android:id="@+id/pregunta2_cero"
            android:layout_width="78dp"
            android:layout_height="61dp"
            android:src="@drawable/respuestas_cero"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pregunta3_cinco" />

        <ImageView
            android:id="@+id/pregunta3_uno"
            android:layout_width="78dp"
            android:layout_height="61dp"
            android:layout_marginTop="56dp"
            android:src="@drawable/respuestas_uno"
            app:layout_constraintEnd_toStartOf="@+id/pregunta3_dos"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
